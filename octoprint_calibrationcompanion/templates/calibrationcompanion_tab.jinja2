        <div class="tab-content">
            <!--Setup-->
            <div class="tab-pane active" id="setup-calibrationcompanion">
                <form class="form-horizontal">
                    <div class="row-fluid">
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label" for="printerName"><strong>Printer Name</strong></label>
                                <div class="controls">
                                    <input type="text" class="midsizeTextbox" id="printerName" data-bind="value: printer_name">
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" for="filamentUsed"><strong>Filament Used</strong></label>
                                <div class="controls">
                                    <select class="midsizeTextbox" id="filamentUsed" data-bind="value: filament_used">
                                        <option value="PLA">PLA</option>
                                        <option value="ABS">ABS</option>
                                        <option value="PETG">PETG</option>
                                        <option value="TPU">TPU</option>
                                    </select>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" for="filamentDiameter"><strong>Filament Diameter</strong></label>
                                <div class="controls">
                                    <div class="input-append">
                                        <input type="text" class="midsizeTextbox" id="filamentDiameter" data-bind="value: filament_diameter" placeholder="(mm)">
                                        <span class="add-on">mm</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label" for="bedSizeX"><strong>Bed Size X</strong></label>
                                <div class="controls">
                                    <div class="input-append">
                                        <input type="text" class="midsizeTextbox" id="bedSizeX" placeholder="X >=100" data-bind="textInput: bed_size_x">
                                        <span class="add-on">mm</span>
                                    </div>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" for="bedSizeY"><strong>Bed Size Y</strong></label>
                                <div class="controls">
                                    <div class="input-append">
                                        <input type="text" class="midsizeTextbox" id="bedSizeY" placeholder="Y >=100" data-bind="textInput: bed_size_y">
                                        <span class="add-on">mm</span>
                                    </div>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" for="bedSizeZ"><strong>Bed Size Z</strong></label>
                                <div class="controls">
                                    <div class="input-append">
                                        <input type="text" class="midsizeTextbox" id="bedSizeZ" data-bind="textInput: bed_size_z">
                                        <span class="add-on">mm</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row-fluid">
                        <div class="span4">
                            <div class="control-group">
                                <label class="control-label" for="nozzleSize"><strong>Nozzle size</strong></label>
                                <div class="controls">
                                    <select class="midsizeTextbox" id="nozzleSize" data-bind="value: nozzle_size">
                                        <option value="0.4">0.4</option>
                                        <option value="0.6">0.6</option>
                                        <option value="0.8">0.8</option>
                                    </select>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" for="relativePositioning"><strong>Relative Positioning</strong></label>
                                <div class="controls">
                                    <input type="checkbox" id="relativePositioning" data-bind="checked: relative_positioning">
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" for="originCheck"><strong>Delta printer (0,0 at centre)</strong></label>
                                <div class="controls">
                                    <div class="input-append">
                                        <input type="checkbox" id="originCheck" data-bind="checked: origin_check">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <!--Profiles Tab-->
            <div class="tab-pane" id="profiles-calibrationcompanion">
                <form class="form-inline">
                    <span id="regular-form-title"><h4><b>Profile</b></h4></span><br>
                        <div class="control-group">
                            <label class="control-label" for="profile-selection"><strong>Profile selection</strong></label>
                            <div class="controls">
                                <div class="input-append">
                                    <select class="form-control" id="profile-selection" data-bind="value: profile_selection">
                                        <option value=""></option>
                                        <option value="profile1">Profile 1</option>
                                        <option value="profile2">Profile 2</option>
                                        <option value="profile3">Profile 3</option>
                                    </select>
                                    <button type="button" class="btn btn-primary" id="load-profile">Load</button>
                                    <button type="button" class="btn btn-primary" id="save-profile">Save</button>
                                    <button type="button" class="btn btn-primary" id="reset-profile">Reset</button>
                                </div>
                            </div>
                        </div>
                </form><br><br><br><br>
                <form class="form-horizontal">
                    <div class="row-fluid">
                        <div class="span6">
                    <span id="regular-form-title"><h4><b>Temperature</b></h4></span><br>
